{
   "background": {
      "scripts": [ "blocked.js" ]
   },
   "content_scripts": [
      {
         "matches": ["\u003Call_urls>"],
         "js": ["inject.js"],
         "css": ["inject.css"],
         "run_at": "document_end"
      }
   ],
   "browser_action": {
      "default_icon": "icon.png",
      "default_title": "Block some website",
      "default_popup": "popup.html"
   },
   "description": "Block some website.",
   "manifest_version": 2,
   "name": "Blocked",
   "permissions": [ "webRequest", "webRequestBlocking", "\u003Call_urls>", "tabs" ],
   "version": "0.0.3"
}
